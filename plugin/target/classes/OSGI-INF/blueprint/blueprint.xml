<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="
        http://www.osgi.org/xmlns/blueprint/v1.0.0
        http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd
        ">
  <!-- Resource types -->
  <service interface="org.opennms.integration.api.v1.config.datacollection.ResourceTypesExtension">
    <bean class="org.opennms.plugins.config.DellDataDomainResourceTypesExtension"/>
  </service>

  <!-- SNMP Datacollection -->
  <service interface="org.opennms.integration.api.v1.config.datacollection.SnmpCollectionExtension">
    <bean class="org.opennms.plugins.config.DellDataDomainSnmpCollectionExtensionImpl"/>
  </service>

  <!-- Graphs -->
  <service interface="org.opennms.integration.api.v1.config.datacollection.graphs.GraphPropertiesExtension">
    <bean class="org.opennms.plugins.config.DellDataDomainGraphPropertiesExtension"/>
  </service>
</blueprint>
