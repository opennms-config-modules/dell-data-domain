      <storageStrategy class="org.opennms.netmgt.collection.support.IndexStorageStrategy"/>
   </resourceType>
   <resourceType name="fileSystemSpaceEntry" label="Dell Data Domain - File System" resourceLabel="${fileSysteResourName}">
